{
  "name": "vue-dynamic-table",
  "version": "1.0.0",
  "private": true,
  "description": "A table front-end plugin for your api with support to all CRUD operations",
  "author": "David Moseler",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build-storybook": "vue-cli-service storybook:build -c config/storybook",
    "storybook": "vue-cli-service storybook:serve -p 6006 -c config/storybook"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "material-design-icons": "^3.0.1",
    "materialize-css": "^1.0.0",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-knobs": ">=5.3.0",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addon-notes": ">=5.3.0",
    "@storybook/addons": "^5.3.19",
    "@storybook/vue": "^5.3.19",
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-eslint": "~4.4.0",
    "@vue/cli-service": "~4.4.0",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-preset-vue": "^2.0.2",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-vue": "^6.2.2",
    "vue-cli-plugin-storybook": "~1.2.2",
    "vue-loader": "^15.9.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/airbnb"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-plusplus": 0,
      "eqeqeq": 0
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "engines": {
    "node": "12.x",
    "npm": "6.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/davidmoseler/vue-dynamic-table.git"
  }
}
